# LinkedIn job search — set country and job title (intitulé du poste), or override with full URL
# Pays de recherche / Job search country (e.g. Canada, France, Morocco)
JOB_SEARCH_COUNTRY=Canada
# Intitulé du poste / Job title or keywords (e.g. software engineer java, développeur full stack)
JOB_SEARCH_KEYWORDS=software engineer java
# Optional: full search URL (if set, JOB_SEARCH_COUNTRY and JOB_SEARCH_KEYWORDS are ignored)
# LINKEDIN_JOB_SEARCH_URL=https://www.linkedin.com/jobs/search/?keywords=...&location=...&f_AL=true

# Easy Apply: email to select in contact step dropdown (default shown in form is not used)
EASY_APPLY_EMAIL=abdellah-lakhnigue@outlook.fr
# First name / Prénom — for form fields (English and French)
EASY_APPLY_FIRST_NAME=
# Last name / Nom — for form fields (English and French)
EASY_APPLY_LAST_NAME=
# Default years fallback (experience map overrides per tech)
EASY_APPLY_YEARS_DEFAULT=3
# CV PDF for experience map + Gemini extraction (required for full auto-fill)
CV_PATH=d:/ATS CV/Abdellah Lakhnigue - Software engineer.pdf

# Gemini API: exact answers from CV for open-ended questions (subscription key)
GEMINI_API_KEY=your_gemini_api_key_here
# Use gemini-2.0-flash (gemini-1.5-flash can 404 with current SDK)
GEMINI_MODEL=gemini-2.0-flash
USE_GEMINI_FOR_CV=true

# Default city for "Location (city)*" (text input or dropdown)
DEFAULT_LOCATION_CITY=Casablanca
# Notice period before start date (CDI norm in Morocco)
NOTICE_PERIOD_DEFAULT=3 months

# Current job (Easy Apply "Current Company *" / "Current Title *"); leave empty to use N/A
EASY_APPLY_CURRENT_COMPANY=
EASY_APPLY_CURRENT_TITLE=
# Gender / sex question: Male, Female, Other (or exact option text from the form)
EASY_APPLY_GENDER=Male
# Certifications: comma-separated (e.g. AWS, Azure) — used for "do you have certificate: X?"; also checked in CV
EASY_APPLY_CERTIFICATIONS=
# Hybrid / remote / work mode questions (e.g. "Are you able to work in hybrid mode?"): Yes or No
EASY_APPLY_HYBRID_ANSWER=Yes

# Work authorization: not authorized in search country; need sponsorship
WORK_AUTHORIZATION_ANSWER=No
WORK_NEED_SPONSORSHIP_ANSWER=Yes
WORK_AUTHORIZATION_COUNTRY=Canada

# Local path for the physical browser upload
RESUME_PATH=./Abdellah Lakhnigue - Software engineer.pdf

# Human-like behavior to avoid bans
MIN_DELAY_SEC=10
MAX_DELAY_SEC=30
HEADLESS=false
